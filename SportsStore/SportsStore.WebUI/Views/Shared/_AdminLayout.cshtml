@{ 
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-theme.css" rel="stylesheet" />
    <link href="~/Content/ErrorStyles.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.4.1.js"></script> @*client validation*@
    <script src="~/Scripts/jquery.validate.js"></script> @*client validation*@
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script> @*client validation*@
    <title></title>
</head>
<body>
    <div>
        @if (TempData["ActionMessage"] != null) 
        {
            <div class="alert alert-success">@TempData["ActionMessage"]</div>
        }
        @base.RenderBody()
    </div>
</body>
</html>
